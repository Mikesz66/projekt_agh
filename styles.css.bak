#mainWindow {
  background-color: #333;
}

QWidget {
  background-color: #333;
}

QLabel {
  color: #eee;
  font-size: 16px;
  font-family: Arial, sans-serif;
}

QPushButton {
  background-color: #555;
  color: white;
  border: 1px solid #666;
  padding: 10px;
  border-radius: 5px;
  font-size: 14px;
  min-width: 20px;
  min-height: 20px;
}

QPushButton:hover {
  background-color: #6a6a6a;
  border: 1px solid #777;
}

#test {
  background-color: #28a745;
  font-weight: bold;
  min-width: 20px;
  min-height: 20px;
}

QScrollArea {
  background-color: #333;
  color: white;
  border: none;
  padding: 5px 10px;
  border-radius: 3px;
}
